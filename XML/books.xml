<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE BOOKS[
<!ELEMENT (book)>
<!ELEMENT book (title)?>
<!ELEMENT book (isbn)+>
<!ELEMENT book (zipcode | author)>
<!ELEMENT book (publish)*>
<!ELEMENT title (#PCDATA)> 
<!ELEMENT isbn (#PCDATA)> 
<!ELEMENT zipcode (#PCDATA)> 
<!ELEMENT author (#PCDATA)> 
<!ELEMENT publish (#PCDATA)> 
<!ATTLIST title name CDATA #IMPLIED>
<!ATTLIST isbn no CDATA #REQUIRED>
<!ATTLIST zipcode Zname CDATA #FIXED "232232323">
<!ATTLIST publish year CDATA #FIXED "1995">
]>
<BOOKS>
    <book>
        <TITLE name="Title of the Book">Home Alone</TITLE>
        <ISBN no="1234567890">978-1234567890</ISBN>
        <ZIPCODE>12345</ZIPCODE>
        <PUBLISH year="1995">Todd Strasser</PUBLISH>
    </book>
</BOOKS>
